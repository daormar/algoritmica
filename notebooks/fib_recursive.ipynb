   "source": [
    "def fibonacci(n):\n",
    "    # Verifica si se ha solicitado alguno de los términos iniciales\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    elif n == 1:\n",
    "        return 1\n",
    "    # No se ha solicitado un término inicial\n",
    "    return fibonacci(n - 1) + fibonacci(n - 2)"
   ]
  }
